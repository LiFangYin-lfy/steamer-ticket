/**app.wxss**/
page,
view,
text,
swiper,
swiper-item,
navigator,
image {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
image {
  width: 100%;
  height: 100%;
}
text {
  font-family: PingFang-SC-Bold;
  color: #06121E;
}
page {
  overflow-x: hidden;
}
.input_placeholder {
  color: #BDC4CE;
  font-size: 24rpx;
}
.textarea_placeholder {
  color: #C8C9CC;
  font-size: 24rpx;
}
/**
去除横向滚动条
*/
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.title_content3 {
  color: #06121f;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.title_content1 {
  color: #06121f;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.title_content2 {
  color: #06121f;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
