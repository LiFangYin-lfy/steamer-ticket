page {
  background: #fff;
  width: 100%;
  height: 100%;
}
.container {
  width: 100%;
}
.container .header {
  width: 100%;
  background: linear-gradient(to right, #ffb94c, #ff893e);
}
.container .header .left_img {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 0 32rpx;
  box-sizing: border-box;
  height: 88rpx;
}
.container .header .left_img image {
  width: 32rpx;
  height: 32rpx;
}
.container .header .left_img .title_oage {
  flex: 1;
  margin-right: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-weight: 500;
}
.container .health {
  width: 100%;
}
.container .health .list_cate {
  width: 100%;
  padding: 32rpx 0;
  box-sizing: border-box;
}
.container .health .list_cate .del_img {
  width: 100%;
  height: 48rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding: 0 32rpx;
  box-sizing: border-box;
}
.container .health .list_cate .del_img image {
  width: 40rpx;
  height: 40rpx;
}
.container .health .list_cate .value_item {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
  box-sizing: border-box;
  border-bottom: 1rpx solid #EEEEEE;
}
.container .health .list_cate .value_item .change_img {
  padding: 32rpx 20rpx 32rpx 32rpx;
  box-sizing: border-box;
  height: 180rpx;
}
.container .health .list_cate .value_item .change_img image {
  width: 32rpx;
  height: 32rpx;
  flex-shrink: 0;
}
.container .health .list_cate .value_item .bos_cart {
  flex: 1;
  display: flex;
}
.container .health .list_cate .value_item .bos_cart .left_image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 8rpx;
  flex-shrink: 0;
  position: relative;
}
.container .health .list_cate .value_item .bos_cart .left_image .item_img {
  width: 100%;
  border-radius: 8rpx;
}
.container .health .list_cate .value_item .bos_cart .left_image .newpin {
  width: 86rpx;
  height: 36rpx;
  opacity: 0.2;
  background: #000000;
  border-radius: 8rpx 0rpx 14rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.container .health .list_cate .value_item .bos_cart .left_image .newpin .vipim {
  width: 100%;
  font-size: 22rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: center;
  color: #ffffff;
  line-height: 36rpx;
}
.container .health .list_cate .value_item .bos_cart .right_cart {
  flex: 1;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.container .health .list_cate .value_item .bos_cart .right_cart .top_carts {
  width: 100%;
  height: 180rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.container .health .list_cate .value_item .bos_cart .right_cart .top_carts .titles {
  width: 100%;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Medium;
  font-weight: 500;
  text-align: left;
  color: #333333;
  line-height: 40rpx;
}
.container .health .list_cate .value_item .bos_cart .right_cart .top_carts .piao_c {
  width: 100%;
  font-size: 24rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #808080;
  line-height: 36rpx;
  padding-bottom: 20rpx;
  box-sizing: border-box;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts .pt {
  display: flex;
  align-items: baseline;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts .pt .pt_icon {
  font-size: 24rpx;
  color: #FF4444;
  font-weight: 500;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts .pt .pt_price {
  font-size: 32rpx;
  color: #FF4444;
  font-weight: 500;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts .rig {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts .rig .img_j {
  width: 48rpx;
  height: 48rpx;
  background-color: #ccc;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts .rig .img_j image {
  width: 100%;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts .rig .img_2 {
  background: #f5f5f5;
  height: 48rpx;
}
.container .health .list_cate .value_item .bos_cart .right_cart .down_carts .rig .img_2 .count {
  font-size: 24rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color: #323233;
  text-align: center;
  height: 48rpx;
  line-height: 48rpx;
}
.container .health .cno {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .health .cno .kongbox {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 200rpx auto;
}
.container .health .cno .kongbox image {
  width: 500rpx;
}
.container .health .cno .kongbox .c_name {
  margin-top: 30rpx;
  font-size: 32rpx;
  color: #ccc;
}
.container .downboxs {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
}
.container .downboxs .wai_boss {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 16rpx 32rpx 16rpx 0;
  box-sizing: border-box;
}
.container .downboxs .wai_boss .boxs_do {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 72rpx;
}
.container .downboxs .wai_boss .boxs_do .ld_all {
  display: flex;
  align-items: center;
  padding-left: 32rpx;
  box-sizing: border-box;
}
.container .downboxs .wai_boss .boxs_do .ld_all image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 16rpx;
}
.container .downboxs .wai_boss .boxs_do .ld_all .all_name {
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #7d7e80;
  line-height: 40rpx;
}
.container .downboxs .wai_boss .boxs_do .rih {
  display: flex;
  align-items: center;
  justify-content: flex-end;
}
.container .downboxs .wai_boss .boxs_do .rih .heji {
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #7d7e80;
  line-height: 40rpx;
  margin-right: 20rpx;
}
.container .downboxs .wai_boss .boxs_do .rih .pt {
  display: flex;
  align-items: baseline;
}
.container .downboxs .wai_boss .boxs_do .rih .pt .pt_icon {
  font-size: 24rpx;
  color: #FF4444;
  font-weight: 500;
}
.container .downboxs .wai_boss .boxs_do .rih .pt .pt_price {
  font-size: 32rpx;
  color: #FF4444;
  font-weight: 500;
}
.container .downboxs .wai_boss .suan {
  width: 200rpx;
  height: 72rpx;
  background: #ffa230;
  border-radius: 36rpx;
  margin-left: 32rpx;
}
.container .downboxs .wai_boss .suan .suan_name {
  width: 100%;
  height: 72rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #ffffff;
}
