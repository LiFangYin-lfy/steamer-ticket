page {
  background: #fff;
  width: 100%;
  height: 100%;
}
.container {
  width: 100%;
}
.container .header {
  width: 100%;
  background: linear-gradient(136deg, #ffbb4d 1%, #ff863d 99%);
}
.container .header .left_img {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 0 32rpx;
  box-sizing: border-box;
  height: 88rpx;
}
.container .header .left_img image {
  width: 32rpx;
  height: 32rpx;
}
.container .header .left_img .title_oage {
  flex: 1;
  margin-right: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-weight: 500;
}
.container .health {
  width: 100%;
}
.container .health .searchbox {
  height: 108rpx;
  width: 100%;
  display: flex;
  align-items: center;
  padding: 0 32rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.container .health .searchbox .pic_names {
  width: 68rpx;
  height: 68rpx;
  margin-left: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .health .searchbox .searchinput {
  flex: 1;
  display: flex;
  align-items: center;
  height: 68rpx;
  background: #f7f8fa;
  border-radius: 4rpx;
  padding-left: 32rpx;
  box-sizing: border-box;
  position: relative;
}
.container .health .searchbox .searchinput .searchicon {
  width: 28rpx;
  height: 28rpx;
  margin-right: 12rpx;
}
.container .health .searchbox .searchinput input {
  font-size: 28rpx;
}
.container .health .searchbox .searchinput .del_img {
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  z-index: 3;
}
.container .health .history_search {
  width: 100%;
  padding: 32rpx;
  box-sizing: border-box;
}
.container .health .history_search .biao_ti {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0;
  box-sizing: border-box;
}
.container .health .history_search .biao_ti .his_nam {
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #7d7e80;
  line-height: 40rpx;
}
.container .health .history_search .biao_ti image {
  width: 40rpx;
  height: 40rpx;
}
.container .health .history_search .tory_list {
  width: 100%;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.container .health .history_search .tory_list .sear_name {
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #323233;
  background: #f7f8fa;
  border-radius: 8rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}
.container .health .history_search .tory_list .sear_name:last-child {
  margin-right: 0;
}
.container .health .history_search .tory_list .ited_items {
  display: flex;
  align-items: center;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
  justify-content: center;
  border-radius: 8rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  background: #f7f8fa;
}
.container .health .history_search .tory_list .ited_items image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}
.container .health .history_search .tory_list .ited_items .sear_nade {
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #323233;
  background: #f7f8fa;
}
