  align-items: center;
  justify-content: center;
  color: #fff;
  font-weight: 500;
}
.container .health {
  width: 100%;
  margin-top: 20rpx;
}
.container .health .nav_title {
  width: 100%;
  padding: 12rpx 32rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.container .health .nav_title .nav_bot {
  width: 100%;
  display: flex;
  align-items: center;
  height: 64rpx;
  background: #ffffff;
  border: 1rpx solid #ffb740;
  border-radius: 32rpx;
}
.container .health .nav_title .nav_bot .box_nav {
  flex: 1;
}
.container .health .nav_title .nav_bot .box_nav .nav_name {
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #FFB740;
  font-size: 32rpx;
  height: 64rpx;
}
.container .health .nav_title .nav_bot .nav_cllass {
  border-radius: 32rpx 0 0 32rpx;
  background: linear-gradient(136deg, #ffbb4d 1%, #ff863d 99%);
}
.container .health .nav_title .nav_bot .nav_cllass .nav_name {
  color: #fff;
}
.container .health .nav_title .nav_bot .nav_cllass2 {
  background: linear-gradient(136deg, #ffbb4d 1%, #ff863d 99%);
  border-radius: 0 32rpx 32rpx 0;
}
.container .health .nav_title .nav_bot .nav_cllass2 .nav_name {
  color: #fff;
}
.container .health .tuan_order {
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  border-top: 1rpx solid #f9f9f9;
}
.container .health .tuan_order .bottom_scroll {
  display: flex;
  align-items: center;
}
.container .health .tuan_order .bottom_scroll .nav {
  line-height: 80rpx;
  flex: 1;
}
.container .health .tuan_order .bottom_scroll .nav .nav_tab {
  color: #06121E;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .health .tuan_order .bottom_scroll .nav .active {
  color: #FFA230;
  font-weight: bold;
  line-height: 80rpx;
  position: relative;
}
.container .health .tuan_order .bottom_scroll .nav .active::after {
  position: relative;
  display: block;
  content: '';
  width: 40rpx;
  height: 4rpx;
  background: #FFA230;
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}
.container .health .tuan_order .bottom_scroll .nav:last-child {
  flex: none;
  width: 200rpx;
}
