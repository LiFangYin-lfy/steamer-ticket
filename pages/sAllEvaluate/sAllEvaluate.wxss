page {
  background: #F3F4F6;
  width: 100%;
  height: 100%;
}
.container {
  width: 100%;
}
.container .header {
  width: 100%;
  background: linear-gradient(to right, #ffb94c, #ff893e);
}
.container .header .left_img {
  width: 100%;
  display: flex;
  align-items: center;
  padding: 0 32rpx;
  box-sizing: border-box;
  height: 88rpx;
}
.container .header .left_img image {
  width: 32rpx;
  height: 32rpx;
}
.container .header .left_img .title_oage {
  flex: 1;
  margin-right: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-weight: 500;
}
.container .health {
  width: 100%;
}
.container .health .bs {
  width: 100%;
}
.container .health .bs .prise {
  width: 100%;
  padding: 28rpx 32rpx;
  box-sizing: border-box;
}
.container .health .bs .prise .pr_list {
  width: 100%;
  margin-bottom: 20rpx;
}
.container .health .bs .prise .pr_list .top_head {
  width: 100%;
  display: flex;
  align-items: center;
}
.container .health .bs .prise .pr_list .top_head .head_img {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  flex-shrink: 0;
}
.container .health .bs .prise .pr_list .top_head .hed_right {
  padding-left: 16rpx;
  box-sizing: border-box;
  height: 72rpx;
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.container .health .bs .prise .pr_list .top_head .hed_right .hed_nam {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.container .health .bs .prise .pr_list .top_head .hed_right .hed_nam .h_names {
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  line-height: 40rpx;
  letter-spacing: 1rpx;
}
.container .health .bs .prise .pr_list .top_head .hed_right .hed_nam .star {
  display: flex;
  align-items: center;
}
.container .health .bs .prise .pr_list .top_head .hed_right .hed_nam .star .iims {
  display: flex;
  align-items: center;
}
.container .health .bs .prise .pr_list .top_head .hed_right .hed_nam .star .iims .lims {
  margin: 0 5rpx;
  width: 26rpx;
  height: 26rpx;
}
.container .health .bs .prise .pr_list .top_head .hed_right .hed_nam .star .tes_star {
  font-size: 18rpx;
  font-family: AlibabaPuHuiTi-Regular;
  color: #fd9400;
  line-height: 22rpx;
  margin-left: 20rpx;
}
.container .health .bs .prise .pr_list .top_head .hed_right .date {
  font-size: 24rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: right;
  color: #999999;
  line-height: 34rpx;
  letter-spacing: 1rpx;
}
.container .health .bs .prise .pr_list .pr_tst {
  margin: 20rpx 0;
  width: 100%;
  font-size: 24rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  line-height: 40rpx;
  letter-spacing: 1rpx;
}
.container .health .bs .prise .pr_list .pr_imgs {
  width: 100%;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
}
.container .health .bs .prise .pr_list .pr_imgs .imgst {
  width: 180rpx;
  height: 180rpx;
  border-radius: 8rpx;
  margin-right: 12rpx;
  margin-bottom: 12rpx;
  flex-shrink: 0;
}
.container .health .bs .sts {
  width: 100%;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: center;
  color: #999999;
  line-height: 34rpx;
  letter-spacing: 1rpx;
}
.container .health .cno {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .health .cno .kongbox {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 200rpx auto;
}
.container .health .cno .kongbox image {
  width: 500rpx;
}
.container .health .cno .kongbox .c_name {
  margin-top: 30rpx;
  font-size: 32rpx;
  color: #ccc;
}
